<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sn_aia_tool">
    <sn_aia_tool action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>This tool enumerates the full name of the currently logged in user.</description>
        <input_schema>[]</input_schema>
        <name>Get Currently Logged In User</name>
        <record_type>custom</record_type>
        <script><![CDATA[(function(inputs) {
    var userGR = new GlideRecord('sys_user');
    if (userGR.get(gs.getUserID())) {
        return {
            name: userGR.name.toString()
        };
    } else {
        return {
            name: ''
        };
    }
})(inputs);]]></script>
